{
    "name": "aws-swf",
    "version": "1.0.2",
    "description": "Node.js toolkit for the Amazon SWF API",
    "author": "Eric Abouaf <eric.abouaf@gmail.com>",
    "preferGlobal": "true",
    "engines": {
        "node" : ">=0.6.0"
    },
    "keywords": [
        "cli",
        "amazon",
        "aws",
        "swf",
        "workflow"
    ],
    "main" : "index.js",
    "directories" : { "lib" : "./lib" },
    "repository": {
        "type":"git",
        "url":"http://github.com/neyric/aws-swf.git"
    }, 
    "bin": {
        "swf-activity": "./cli/swf-activity.js",
        "swf-decider": "./cli/swf-decider.js",
        "swf-register": "./cli/swf-register.js",
        "swf-set-credentials": "./cli/swf-set-credentials.js",
        "swf-start": "./cli/swf-start.js"
    },
    "scripts": {
        "install": "./cli/swf-set-credentials.js"
    },
    "dependencies": {
            "optimist": "0.3.4",
            "async": "0.1.22",
            "colors": "0.6.0-1"
    },
    "licenses" : [ 
        { 
            "type" : "MIT", 
            "url" : "https://raw.github.com/neyric/aws-swf/master/LICENSE.txt"
        }
    ]
}